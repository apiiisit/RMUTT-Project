{
  "private": true,
  "main": "app/index.js",
  "scripts": {
    "start": "npm-run-all -p start:web start:app",
    "start:web": "ng serve --proxy-config proxy.conf.json",
    "start:app": "wait-on tcp:4200 && npm run build:electron && electron . --serve",
    "build": "npm run build:web && npm run build:app",
    "build:web": "ng build --base-href ./",
    "build:app": "npm run build:electron && electron-builder build --publish=never",
    "build:electron": "tsc -p tsconfig.serve.json"
  },
  "dependencies": {
    "@angular/animations": "^15.1.0",
    "@angular/common": "^15.1.0",
    "@angular/compiler": "^15.1.0",
    "@angular/core": "^15.1.0",
    "@angular/forms": "^15.1.0",
    "@angular/platform-browser": "^15.1.0",
    "@angular/platform-browser-dynamic": "^15.1.0",
    "@angular/router": "^15.1.0",
    "jspdf": "^2.5.1",
    "jspdf-autotable": "^3.5.28",
    "primeflex": "^3.3.0",
    "primeicons": "^6.0.1",
    "primeng": "^15.1.1",
    "rxjs": "~7.8.0",
    "sweetalert2": "^11.7.1",
    "tslib": "^2.3.0",
    "xlsx": "^0.18.5",
    "zone.js": "~0.12.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.1.2",
    "@angular/cli": "~15.1.2",
    "@angular/compiler-cli": "^15.1.0",
    "@types/jasmine": "~4.3.0",
    "electron": "^22.0.3",
    "electron-builder": "^23.6.0",
    "jasmine-core": "~4.5.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "npm-run-all": "^4.1.5",
    "typescript": "~4.9.4",
    "wait-on": "^7.0.1"
  }
}
